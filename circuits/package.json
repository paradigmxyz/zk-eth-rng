{
  "name": "ZKPBlockhashes",
  "license": "GPL-3.0",
  "version": "0.0.1",
  "description": "Attestations to Ethereum block header values",
  "main": "index.js",
  "repository": "git@github.com:amangotchu/randaozkp.git",
  "author": "Aman Gottumukkala <amangotchu@users.noreply.github.com>",
  "scripts": {
    "compile": "circom main.circom --r1cs --c --verbose",
    "gen_witness": "node ./main_js/generate_witness.js ./main_js/main.wasm ./witness_generation/input.json ./witness_generation/witness.wtns",
    "gen_verification_key": "cd ./trusted_setup && ./gen_verification_key.sh && cd ../",
    "gen_proof": "snarkjs groth16 prove --verbose ./trusted_setup/singleBlockHeader.zkey ./build/singleBlockheader/witness.wtns ./build/singleBlockheader/proof.json ./build/singleBlockheader/public.json",
    "verify_proof": "snarkjs groth16 verify ./trusted_setup/vkey.json ./proofs/public.json ./proofs/proof.json"
  },
  "dependencies": {
    "axios": "^1.2.1",
    "circomlib": "^2.0.2",
    "dotenv": "^16.0.3",
    "ethers": "^5.7.2",
    "jsonfile": "^6.1.0",
    "minimist": "^1.2.7",
    "snarkjs": "^0.4.15",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "devDependencies": {
    "@types/node": "^18.11.16"
  }
}
